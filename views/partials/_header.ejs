<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-info">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
       </div>
    </div>
    <% if(typeof user !== 'undefined' && user){ %>
      <h1> <form action='/logout' method='POST'>
      </h1>
       <p class="text-white">Logged in as: <%= user.email %></p>
        <div class="col-auto">
          <label class="sr-only" for="inlineFormInputGroup">Username</label>
          <div class="input-group mb-2">
            <div class="input-group-prepend">
            </div>
            <button type="submit" class="btn btn-secondary mb-2">logout</button>
          </div>
      </div>
    <% } else{ %>  
    <div class="navbar-nav">
      <h1> <a class="nav-item nav-link class=btn btn-secondary mb-2" href="/register">Register Account</a>
        <a class="nav-item nav-link class=btn btn-secondary mb-2" href="/login">Login</a>
    </form>
   <% } %>
    </div>
  </nav>
</header>